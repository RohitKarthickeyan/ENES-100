#include "Arduino.h"


int in1 = 10;
int in2 = 11;


int in3 = 12;
int in4 = 13;

void setup() 
{
  Serial.begin(9600);
  pinMode(10, OUTPUT);
  pinMode(11, OUTPUT);
  pinMode(12, OUTPUT);
  pinMode(13, OUTPUT);

}

void loop() 

{
  
  digitalWrite(in1, HIGH); 
  digitalWrite(in2, LOW); 
  
  
  digitalWrite(in3, LOW);
  digitalWrite(in4, HIGH);
  
}
